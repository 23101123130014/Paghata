#include <iostream>
using namespace std;
int main()
{
	int n1, n2, sw;
	n1 = 23;
	n2 = 43;
	cout<<"before swap n1 = "<<n1;
	cout<<"\n before swap n2 = " <<n2;
	cout<<endl;
	sw = n1;
	n1 = n2;
	n2 = sw;
	cout<<"\n after swaping the value of n1 = " <<n1;
	cout<<"\n after the value of n1 = " <<n1;
	return 0;
}
